{
  "name": "xstream-store",
  "version": "0.0.0-semantically-released",
  "description": "A redux-like store for xstream",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "changelog": "conventional-changelog --infile CHANGELOG.md --same-file --release-count 0 --preset angular",
    "test": "jest",
    "watch:test": "jest --watch",
    "semantic-release": "semantic-release",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fixate/xstream-store.git"
  },
  "keywords": [
    "xstream"
  ],
  "author": "Larry Botha",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fixate/xstream-store/issues"
  },
  "homepage": "https://github.com/fixate/xstream-store#readme",
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "commitizen": "^2.10.1",
    "conventional-changelog": "^2.0.0",
    "conventional-changelog-cli": "^2.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "ghooks": "^2.0.4",
    "jest": "^23.0.1",
    "prettier": "^1.13.0",
    "semantic-release": "^15.5.0",
    "travis-deploy-once": "^5.0.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^6.0.5",
    "typescript": "^2.8.3"
  },
  "peerDependencies": {
    "xstream": "^11.4.0"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
